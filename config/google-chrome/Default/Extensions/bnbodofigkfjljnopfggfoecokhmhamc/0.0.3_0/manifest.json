{
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "Automatically rejects cookie popups on websites",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Reject Cookies",
   "permissions": [ "activeTab", "sidePanel", "tabs" ],
   "side_panel": {
      "default_path": "sidepanel.html"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.3"
}
